{
  "name": "council-of-elders",
  "version": "0.3.1",
  "description": "Model Context Protocol (MCP) server and CLI that queries multiple LLMs through OpenRouter with consensus rounds",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "coe": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "cli": "node dist/cli.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "mcp",
    "llm",
    "openrouter",
    "ai",
    "council-of-elders",
    "consensus"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@openrouter/ai-sdk-provider": "^0.6.0",
    "@types/inquirer": "^9.0.8",
    "@types/lodash-es": "^4.17.12",
    "ai": "^4.3.16",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^16.4.5",
    "enquirer": "^2.4.1",
    "inquirer": "^12.6.3",
    "lodash-es": "^4.17.21",
    "node-fetch": "^3.3.2",
    "ora": "^8.1.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "zod": "^3.25.36"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^1.0.0"
  }
